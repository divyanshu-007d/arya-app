{
  "expo": {
    "name": "ARYA - NDA Training App",
    "slug": "arya-nda-training",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "AI-powered NDA preparation app with study tools, fitness tracking, and interview practice",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4169E1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arya"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4169E1"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.arya",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "ARYA - NDA Training",
      "shortName": "ARYA",
      "description": "AI-powered NDA preparation platform",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "themeColor": "#4169E1",
      "backgroundColor": "#FAFAFA"
    },
    "plugins": [
      "expo-web-browser",
      "expo-document-picker",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "ed9ab82a-23bc-47e8-8c72-991fe478dee9"
      }
    }
  }
}
